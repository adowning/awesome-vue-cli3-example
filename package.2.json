{
  "name": "vue-cli-overall-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "bash ./commands/deploy.sh",
    "analyz": "NODE_ENV=production  npm_config_report=true npm run build",
    "clear-commit-msg": "bash ./commands/clear-commit-msg.sh",
    "eslint-fix": "eslint src/**/**/*.vue --fix",
    "format-code": "prettier-eslint --write \"src/**/*.js\" \"src/**/*.vue\"",
    "precommit-msg": "echo 'ðŸ‰ Start pre-commit checks...' && exit 0",
    "preview": "node ./commands/preview.js",
    "start": "yarn serve",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "alertifyjs": "^1.11.2",
    "axios": "^0.18.0",
    "cloudboost": "^2.0.264",
    "dayjs": "^1.6.6",
    "fingerprintjs2": "^1.8.6",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.11",
    "marked": "^0.5.0",
    "material-design-icons-iconfont": "^3.0.3",
    "moment": "^2.23.0",
    "q": "^1.5.1",
    "register-service-worker": "^1.0.0",
    "roboto-fontface": "*",
    "vue": "^2.5.21",
    "vue-meta": "^1.5.4",
    "vue-router": "^3.0.1",
    "vue-signature-pad": "^1.1.6",
    "vue-socket.io": "^3.0.4",
    "vuetify": "^1.3.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.0",
    "@vue/cli-plugin-eslint": "^3.1.0",
    "@vue/cli-plugin-pwa": "^3.1.0",
    "@vue/cli-plugin-unit-jest": "^3.1.0",
    "@vue/cli-service": "^3.1.0",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "hard-source-webpack-plugin": "^0.12.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prerender-spa-plugin": "3.2.0",
    "prettier-eslint-cli": "^4.7.1",
    "size-plugin": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "svg-sprite-loader": "^3.8.0",
    "svgo-loader": "1.1.0",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-svg-loader": "^0.5.0",
    "vue-template-compiler": "^2.5.21",
    "vuetify-loader": "^1.0.5",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "babel": {
    "presets": ["@vue/app"]
  },
  "eslintConfig": {
    "globals": {
      "L": true
    },
    "root": true,
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "no-console": 0,
      "no-useless-escape": 0,
      "no-multiple-empty-lines": [
        2,
        {
          "max": 3
        }
      ],
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "semi": false,
          "trailingComma": "none",
          "bracketSpacing": true,
          "jsxBracketSameLine": true,
          "insertPragma": true,
          "requirePragma": false
        }
      ]
    },
    "plugins": [],
    "extends": [
      "plugin:vue/essential",
      "plugin:prettier/recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": ["> 1%", "last 2 versions", "not ie <= 8"],
  "eslintIgnore": ["package.json"],
  "husky": {
    "hooks": {
      "pre-commit": "yarn run precommit-msg && lint-staged"
    }
  },
  "lint-staged": {
    "**/**.{js,json,pcss,md,vue}": ["prettier --write", "git add"]
  }
}
